    <link rel="apple-touch-icon" sizes="180x180" href="/images/favicon_io/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon_io/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon_io/favicon-16x16.png">
    <link rel="manifest" href="/images/favicon_io/site.webmanifest">
    
  </head>
<body>
    <div class="container mt-5">
        <div class="container">
            <hr>
            <div class="row">
                <div class="col-3 border-end border-1 ps-5">
                    <div class="row-4" >50lac</div>
                    <div class="row-1">Emi</div>
                    <div class="row-1"></div>
                </div>
                
                <div class="col-7 ps-5">
                    <div class="row-4" >2 bhk 4 baths</div>
                    <div class="row-1">Residential Apartment</div>
                    <div class="row-1">in jnl,jnl,mumbai</div>
                </div>
                <div class="col-2">
                    
                    <div class="row-4 p-3"><button type="button"  class="btn btn-outline-warning">Shortlist</button></div>
                    
                </div>
            </div>
        </div>
        <div class="cont">
            <nav>
