                                        <div class="row m-2 pb-4 bg-light">
                                            <div class="col">
                                                <img src="../images/icons8-address-30.png" width=40px height=40px>
                                            </div>
                                            <div class="col">
                                                <div class="row">
                                                    <b>Address</b>
                                                </div>
                                                <div class="row">
                                                    jnl
                                                </div>
                                                <div class="row">
                                                    Mumbai,South
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                
                                
                                    <div class="col">
                                        <div class="row m-2 pb-4 bg-light">
