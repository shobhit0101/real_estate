                                    <div class="carousel-cell"><div class="img"><img src="images\greg-rivers-4vjZfcA4OSY-unsplash.jpg" ></div></div>
                                    <div class="carousel-cell"><div class="img"><img src="images\property_img2.jpg" ></div></div>
                                    <div class="carousel-cell"><div class="img"><img src="images\lucknow.jpg" ></div></div>
                                </div>
                            </div>
                            <div class="col-6 mt-5 ms-3 ">
                                <div class="row row-cols-2 bg-light ">
                                    <div class="col">
                                        <div class="row pt-4 m-2 pb-4 bg-light">
                                            <div class="col">
                                                <img src="../images/parquet.png" width=40px height=40px>
                                            </div>
                                            <div class="col">
                                                <div class="row">
                                                    <b>Area</b>
                                                </div>
                                                <div class="row">
                                                        5000 sq.ft.
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="row pt-4 m-2 pb-4 bg-light">
                                            <div class="col">
                                                <img src="../images/icons8-surface-50.png" width=40px height=40px>
                                            </div>
                                            <div class="col">
                                                <div class="row">
                                                    <b>Configuration</b>
                                                </div>
                                                <div class="row">
                                                    <div class="col-11">
                                                        2 bedroom, 2 bathroom, 3balcony
                                                    </div>
                                                </div>
                                                
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="row m-2 pb-4 bg-light">
                                            <div class="col">
                                                <img src="../images/icons8-price-50.png" width=40px height=40px>
                                            </div>
                                            <div class="col">
