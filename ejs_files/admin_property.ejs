<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial- scale=1.0">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
      <!-- about.css -->
  <link rel="stylesheet" href="css/home.css">
    <title>Perfect View | <%=title %></title>
</head>
<body style="background-color: black;">
    
    <%- include('./partials/admin_nav.ejs'); %>
    <div class="admin_portal_container">
        <pre style="margin: auto;"><b>A D M I N   P O R T A L</b></pre>
  </div>

  <div class="property_table_container_header">
    <div class="row">
      <div class="col-2">
        <b class="p_class" >id</b>
      </div>
      <div class="col-4">
        <b class="p_class" >email</b>
      </div>
      <div class="col-2">
        <b class="p_class" >category</b>
      </div>
      <div class="col-2">
        <b class="p_class" >price</b>
      </div>
      <div class="col-1">
        <b class="p_class" >area</b>
      </div>
      <div class="col-1">
        <b class="p_class" >City</b>
      </div>
</div>
</div>
<% if(data.length>0) { %>
<div class="property_table_container">
    <%data.forEach(property=>{%>  
    <div class="row user_table_row">
        
      <div class="col-2">
        <p class="p_class" ><%= property._id %></p>
      </div>
      <div class="col-4">
        <p class="p_class" ><%= property.email%></p>
      </div>
        <div class="col-2">
        <p class="p_class" ><%= property.prop_cat%></p>
        </div>
        <div class="col-2">
        <p class="p_class" ><%= property.price%></p>
        </div>
        <div class="col-1">
        <p class="p_class" ><%= property.area%></p>
        </div>
        <div class="col-1">
        <p class="p_class" ><%= property.city%></p>
        </div>
    </div>   
      <% }) %>

<% } else { %>
    <div >
        <div class="row">
          <div class="col">
            <b class="p_class" >empty</b>
          </div>
          <div class="col">
            <b class="p_class" >empty</b>
          </div>
          <div class="col">
            <b class="p_class" >empty</b>
            </div>
            <div class="col">
             <b class="p_class" >empty</b>
         </div>
         <div class="col">
          <b class="p_class" >empty</b>
          </div>
          <div class="col">
           <b class="p_class" >empty</b>
       </div>
    </div>
    </div>
    
    <% } %>
</div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>